{
  "Minipage": {
    "prefix": "minipage",
    "body": ["\\begin{minipage}[t]{$0\\textwidth}", "\\end{minipage}%"],
    "description": "Add a table"
  },
  "Frame": {
    "prefix": "frame",
    "body": ["\\begin{frame}", "\t\\frametitle{$1}", "\\end{frame}", "$0"],
    "description": "Add a beamer frame"
  },
  "Code": {
    "prefix": "code",
    "body": ["\\begin{lstlisting}[language=$0]", "\t", "\\end{lstlisting}"],
    "description": "Add a code block"
  },
  "begin": {
    "prefix": "begin",
    "body": ["\\begin{$1}", "\t$0", "\\end{$1}"],
    "description": "Add a begin/end block"
  },
  "item": {
    "prefix": "item",
    "body": ["\\item $0"],
    "description": "Add an item"
  },
  "enumerate": {
    "prefix": "enumerate",
    "body": ["\\begin{enumerate}", "\t\\item $0", "\\end{enumerate}"],
    "description": "Add an enumerate block"
  },

  "template": {
    "prefix": "template",
    "body": [
      "%%%%%%%%%%%%%%%%%%%%%%%%%%%%% Define Article %%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%",
      "\\documentclass[a4paper,12pt]{article}",
      "%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%",
      "",
      "%%%%%%%%%%%%%%%%%%%%%%%%%%%%%% Using Packages %%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%",
      "\\usepackage{geometry}",
      "\\usepackage{graphicx}",
      "\\usepackage{amssymb}",
      "\\usepackage{amsmath}",
      "\\usepackage{amsthm}",
      "\\usepackage{empheq}",
      "\\usepackage{mdframed}",
      "\\usepackage{booktabs}",
      "\\usepackage{lipsum}",
      "\\usepackage{graphicx}",
      "\\usepackage{color}",
      "\\usepackage{xcolor}",
      "\\usepackage{psfrag}",
      "\\usepackage{pgfplots}",
      "\\usepackage{bm}",
      "\\usepackage{listings}",
      "%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%",
      "",
      "%%%%%%%%%%%%%%%%%%%%%%%%%% Page Setting %%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%",
      "\\geometry{a4paper}",
      "",
      "%%%%%%%%%%%%%%%%%%%%%%%%%% Define some useful colors %%%%%%%%%%%%%%%%%%%%%%%%%%",
      "\\definecolor{ocre}{RGB}{243,102,25}",
      "\\definecolor{mygray}{RGB}{243,243,244}",
      "\\definecolor{deepGreen}{RGB}{26,111,0}",
      "\\definecolor{shallowGreen}{RGB}{235,255,255}",
      "\\definecolor{deepBlue}{RGB}{61,124,222}",
      "\\definecolor{shallowBlue}{RGB}{235,249,255}",
      "%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%",
      "",
      "%%%%%%%%%%%%%%%%%%%%%%%%%% Define an orangebox command %%%%%%%%%%%%%%%%%%%%%%%%",
      "\\newcommand\\orangebox[1]{\\fcolorbox{ocre}{mygray}{\\hspace{1em}#1\\hspace{1em}}}",
      "%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%",
      "",
      "%%%%%%%%%%%%%%%%%%%%%%%%%%%% English Environments %%%%%%%%%%%%%%%%%%%%%%%%%%%%%",
      "\\newtheoremstyle{mytheoremstyle}{3pt}{3pt}{\\normalfont}{0cm}{\\rmfamily\\bfseries}{}{1em}{{\\color{black}\\thmname{#1}~\\thmnumber{#2}}\\thmnote{\\,--\\,#3}}",
      "\\newtheoremstyle{myproblemstyle}{3pt}{3pt}{\\normalfont}{0cm}{\\rmfamily\\bfseries}{}{1em}{{\\color{black}\\thmname{#1}~\\thmnumber{#2}}\\thmnote{\\,--\\,#3}}",
      "\\theoremstyle{mytheoremstyle}",
      "\\newmdtheoremenv[linewidth=1pt,backgroundcolor=shallowGreen,linecolor=deepGreen,leftmargin=0pt,innerleftmargin=20pt,innerrightmargin=20pt,]{theorem}{Theorem}[section]",
      "\\theoremstyle{mytheoremstyle}",
      "\\newmdtheoremenv[linewidth=1pt,backgroundcolor=shallowBlue,linecolor=deepBlue,leftmargin=0pt,innerleftmargin=20pt,innerrightmargin=20pt,]{definition}{Definition}[section]",
      "\\theoremstyle{myproblemstyle}",
      "\\newmdtheoremenv[linecolor=black,leftmargin=0pt,innerleftmargin=10pt,innerrightmargin=10pt,]{problem}{Problem}[section]",
      "%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%",
      "",
      "%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%% Plotting Settings %%%%%%%%%%%%%%%%%%%%%%%%%%%%%",
      "\\usepgfplotslibrary{colorbrewer}",
      "\\pgfplotsset{width=8cm,compat=1.9}",
      "%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%",
      "",
      "%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%% Code Block Settings %%%%%%%%%%%%%%%%%%%%%%%%%%%",
      "% Define colors",
      "\\definecolor{greenText}{rgb}{0.5, 0.7, 0.5}    % Green text (comments)",
      "\\definecolor{greyText}{rgb}{0.5, 0.5, 0.5}     % Grey text (line numbers)",
      "\\definecolor{codeFrame}{rgb}{0.5, 0.7, 0.5}    % Green frame around code",
      "\\definecolor{codegreen}{rgb}{0, 0.6, 0}        % Green for comments (override)",
      "\\definecolor{codegray}{rgb}{0.5, 0.5, 0.5}     % Grey for line numbers (override)",
      "\\definecolor{codepurple}{rgb}{0.58, 0, 0.82}   % Purple for strings",
      "\\definecolor{backcolour}{rgb}{0.95, 0.95, 0.92} % Light background",
      "",
      "% Define code settings",
      "\\lstdefinestyle{customcode}{",
      "    backgroundcolor=\\color{backcolour},                      % Background color",
      "    frame=single, rulecolor=\\color{codeFrame},               % Green frame around code",
      "    numbers=left,                                            % Line numbers on the left",
      "    numbersep=8pt,                                           % Space between line numbers and code",
      "    numberstyle=\\tiny\\color{greyText},                     % Line number style (grey and tiny)",
      "    commentstyle=\\color{codegreen},                         % Comments in green",
      "    keywordstyle=\\color{magenta},                           % Keywords in magenta",
      "    stringstyle=\\color{codepurple},                         % Strings in purple",
      "    basicstyle=\\linespread{1.1}\\ttfamily\\footnotesize,    % Code line spacing",
      "    showstringspaces=false,                                  % Do not show spaces in strings",
      "    xleftmargin=1.95em,                                      % Align frame with main text",
      "    framexleftmargin=1.6em,                                  % Extend frame to include line numbers",
      "    breaklines=true,                                         % Wrap long lines",
      "    postbreak=\\mbox{\\textcolor{greenText}{\\$\\hookrightarrow\\$}\\space}, % Mark wrapped lines",
      "    tabsize=2,                                               % Set tab size",
      "    showtabs=false,                                          % Do not show tabs",
      "    captionpos=b                                             % Place caption below code",
      "}",
      "",
      "% Set global code style",
      "\\lstset{style=customcode}",
      "%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%",
      "",
      "%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%% Title & Author %%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%",
      "\\title{Title}",
      "\\author{Wijnants Kobe}",
      "%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%",
      "",
      "\\begin{document}",
      "\t\\maketitle",
      "\t\\newpage",
      "\\end{document}"
    ],
    "description": "Custom LaTeX template for Kobe"
  }
}
