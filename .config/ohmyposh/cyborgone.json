{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "version": 2,
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "template": "\u256d{{ .UserName }}@{{ .HostName }}~ ",
          "background": "",
          "foreground": "#3ce6bf",
          "Text": "",
          "Duration": 0,
          "NameLength": 0
        },
        {
          "type": "path",
          "style": "plain",
          "template": " {{ .Path }} ",
          "properties": {
            "folder_separator_icon": "\\",
            "home_icon": "\u5b85",
            "style": "mixed"
          },
          "background": "",
          "foreground": "#f48fb1",
          "Text": "",
          "Duration": 0,
          "NameLength": 0
        },
        {
          "type": "git",
          "style": "plain",
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{ if gt .StashCount 0 }} \uf692 {{ .StashCount }}{{ end }} ",
          "properties": {
            "branch_identical_icon": "\u2261",
            "fetch_status": true,
            "powerline_symbol": "\ue0b0"
          },
          "background": "",
          "foreground": "#62beff",
          "Text": "",
          "Duration": 0,
          "NameLength": 0
        },
        {
          "type": "python",
          "style": "powerline",
          "template": "[üêç {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }}] ",
          "background": "",
          "foreground": "#b1ff4f",
          "Text": "",
          "Duration": 0,
          "NameLength": 0
        },
        {
          "type": "node",
          "style": "plain",
          "template": "<#ffffff>\u25e6</> ‚ò¢{{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} ",
          "properties": {
            "fetch_version": true
          },
          "background": "",
          "foreground": "#98C379",
          "Text": "",
          "Duration": 0,
          "NameLength": 0
        }
      ],
      "newline": true
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "type": "exit",
          "style": "plain",
          "template": " √ó√ó√ó ",
          "background": "",
          "foreground": "#fb0207",
          "Text": "",
          "Duration": 0,
          "NameLength": 0
        },
        {
          "type": "time",
          "style": "plain",
          "template": " {{ .CurrentDate | date .Format }} ",
          "properties": {
            "time_format": "15:04:05"
          },
          "background": "",
          "foreground": "#81c784",
          "Text": "",
          "Duration": 0,
          "NameLength": 0
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "template": "\u2570# ",
          "background": "",
          "foreground": "#3ce6bf",
          "Text": "",
          "Duration": 0,
          "NameLength": 0
        }
      ],
      "newline": true
    },
    {
      "type": "rprompt",
      "alignment": "right",
      "segments": [
        {
          "type": "executiontime",
          "style": "plain",
          "template": " {{ .FormattedMs }}\u2936",
          "properties": {
            "style": "austin",
            "threshold": 0
          },
          "background": "",
          "foreground": "#4dd0e1",
          "Text": "",
          "Duration": 0,
          "NameLength": 0
        }
      ]
    }
  ]
}
