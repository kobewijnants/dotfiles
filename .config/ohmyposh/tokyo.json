{
	"$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
	"blocks": [
		{
			"alignment": "left",
			"newline": true,
			"segments": [
				{
					"foreground": "#7eb8da",
					"style": "plain",
					"template": "\u250f[<#ffffff>\ueb99</> {{ .UserName }} <#ffffff>\uF303</> {{ .HostName }}]",
					"type": "session"
				},
				{
					"foreground": "#be9ddf",
					"properties": {
						"style": "dallas",
						"threshold": 0
					},
					"style": "diamond",
					"template": "[<#ffffff>\uf252</> {{ .FormattedMs }}s]",
					"type": "executiontime"
				},
				{
					"properties": {
						"root_icon": "\uf292"
					},
					"style": "diamond",
					"template": "\uf0e7",
					"type": "root"
				},
				{
					"foreground": "#ffa5d8",
					"properties": {
						"fetch_stash_count": true,
						"fetch_status": true,
						"fetch_upstream_icon": true
					},
					"style": "plain",
					"template": "[<#ffffff>{{ .UpstreamIcon }}</>{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} <#ffffff>\uf044</> {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} <#ffffff>\uf046</> {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} <#ffffff>\ueb4b</> {{ .StashCount }}{{ end }}]",
					"type": "git"
				},
				{
					"foreground": "#ffa5d8",
					"style": "plain",
					"template": "[\uf0e7]",
					"type": "root"
				},
				{
					"foreground": "#ffa5d8",
					"style": "powerline",
					"template": "[<#ffffff>\uea6c</> Error, check your command]",
					"type": "status"
				}
			],
			"type": "prompt"
		},
		{
			"alignment": "left",
			"newline": false,
			"segments": [
				{
					"foreground": "#7eb8da",
					"style": "plain",
					"template": "[<#98bfad>{{ .Path }}</>]",
					"type": "path"
				}
			],
			"type": "prompt"
		},
		{
			"alignment": "left",
			"newline": true,
			"segments": [
				{
					"foreground": "#7eb8da",
					"properties": {
						"style": "full"
					},
					"style": "plain",
					"template": "\u2516\u2500Δ",
					"type": "text"
				}
			],
			"type": "prompt"
		}
	],
	"transient_prompt": {
		"background": "transparent",
		"foreground": "#7eb8da",
		"template": " Δ "
	},
	"final_space": true,
	"version": 2
}
